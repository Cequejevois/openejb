<% for(var attribute in this.Class.attributes) { %>
	<% if(attribute == 'id' || attribute == 'comments') continue; %>
  <td class='<%= attribute %>'>
	<% if(attribute == 'user') { %>
	  <%= this[attribute].fullname %>
	<% } else { %>
	  <%= this[attribute] %>
  <% } %>
  </td>
<% } %>
<td>
	<a href='javascript://' class='edit'>edit</a>
	<a href='javascript://' class='destroy'>destroy</a>
</td>